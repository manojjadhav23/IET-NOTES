package com.demo.beans;

import java.util.Arrays;

public class Student {
	protected int[] marks;

	public Student() {
		super();
	}

	public Student(int[] marks) {
		super();
		this.marks = marks;
	}

	public int[] getMarks() {
		return marks;
	}

	public void setMarks(int[] marks) {
		this.marks = marks;
	}

	@Override
	public String toString() {
		return "Student [marks=" + Arrays.toString(marks) + "]";
	} 
	
}
