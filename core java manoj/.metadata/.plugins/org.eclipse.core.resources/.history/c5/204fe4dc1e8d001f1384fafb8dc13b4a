package com.demo.beans;

public class MyClass {

	public void PrintTables(int n) {
		for(int i=1;i<11;i++) {
			System.out.println(n+" * "+i+" = "+n*i);
		}
		
	}

	public void Factorial(int a) {
		int fact=1;
		for(int i=1;i<=a;i++) {
			fact=fact*i;
		}
		System.out.println("Factorial of "+a+" is: "+fact);
	}
	
}
