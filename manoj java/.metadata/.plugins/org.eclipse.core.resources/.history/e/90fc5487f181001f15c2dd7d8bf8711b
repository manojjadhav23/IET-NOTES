package Array;

import java.util.Scanner;

public class Array {
	public static void main(String args)
	{
		int[] arr=new int[10];
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Eneter the value");
		for(int i=0;i<arr.length;i++)
		{
			arr[i]=sc.nextInt();
		}
		for(int i=0;i<arr.length;i++) {
			System.out.println("array element"+arr[i]);
		}
              
	}

}